<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CreerCompte">

    <TextView
        android:id="@+id/cr_ez_un_co"
        android:layout_width="395dp"
        android:layout_height="34dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="Créer un compte"
        android:textColor="#EB274B"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Nom prénom -->
    <TextView
        android:id="@+id/nom_et_pr_n"
        android:layout_width="385dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="16dp"
        android:gravity="top"

        android:text="Nom et Prénom"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cr_ez_un_co"
        android:layout_marginLeft="30dp"></TextView>

    <View
        android:id="@+id/rectangle_7"
        android:layout_width="354dp"
        android:layout_height="36dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:width="354dp"
        android:height="36dp"
        android:background="@drawable/rectangle_7"
        android:viewportWidth="354"
        android:viewportHeight="36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nom_et_pr_n" />

    <!-- Adresse mail -->

    <EditText
        android:id="@+id/nom_et_pr_nTExt"
        android:layout_width="350dp"
        android:layout_height="33dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:gravity="top"
        android:hint="Nom et Prénom"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_7"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle_7"
        app:layout_constraintTop_toBottomOf="@+id/nom_et_pr_n"
        app:layout_constraintVertical_bias="0.727" />

    <TextView
        android:id="@+id/adresseMail"
        android:layout_width="382dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"

        android:gravity="top"
        android:text="Adresse mail"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_7" />

    <View
        android:id="@+id/rectangle_7adresseMail"
        android:layout_width="354dp"
        android:layout_height="36dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:width="354dp"
        android:height="36dp"
        android:background="@drawable/rectangle_7"
        android:viewportWidth="354"
        android:viewportHeight="36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adresseMail" />

    <!-- Numéro de téléphone -->

    <EditText
        android:id="@+id/nom_et_pr_nTExtadresseMail"
        android:layout_width="306dp"
        android:layout_height="34dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:gravity="top"
        android:inputType="textEmailAddress"
        android:text="Adresse mail"
        android:textColor="#66000000"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_7adresseMail"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_7adresseMail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle_7adresseMail"
        app:layout_constraintTop_toTopOf="@+id/rectangle_7adresseMail"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/Numero"
        android:layout_width="377dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"

        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:gravity="top"
        android:text="Numéro"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_7adresseMail" />

    <View
        android:id="@+id/rectangle_7Numero"
        android:layout_width="354dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:width="354dp"
        android:height="36dp"
        android:background="@drawable/rectangle_7"
        android:viewportWidth="354"
        android:viewportHeight="36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Numero" />

    <!-- Mot de passe -->

    <EditText
        android:id="@+id/nom_et_pr_nTExtNumero"
        android:layout_width="320dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:gravity="top"
        android:text="Numéro"
        android:textColor="#66000000"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_7Numero"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_7Numero"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle_7Numero"
        app:layout_constraintTop_toTopOf="@+id/rectangle_7Numero"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/MotDePasse"
        android:layout_width="387dp"
        android:layout_height="24dp"

        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:gravity="top"
        android:text="Mot de passe"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_7Numero" />

    <View
        android:id="@+id/rectangle_7MotDePasse"
        android:layout_width="354dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:width="354dp"
        android:height="36dp"
        android:background="@drawable/rectangle_7"
        android:viewportWidth="354"
        android:viewportHeight="36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MotDePasse" />

    <!-- Répéter le mot de passe -->

    <EditText
        android:id="@+id/nom_et_pr_nTExtMotDePasse"
        android:layout_width="331dp"
        android:layout_height="34dp"
        android:layout_marginStart="8dp"
        android:gravity="top"
        android:text="Mot de passe"
        android:textColor="#66000000"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_7MotDePasse"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_7MotDePasse"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle_7MotDePasse"
        app:layout_constraintTop_toTopOf="@+id/rectangle_7MotDePasse"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/Répéterlemotdepasse"
        android:layout_width="388dp"
        android:layout_height="25dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:gravity="top"
        android:text="Répétez le mot de passe"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_7MotDePasse" />

    <View
        android:id="@+id/rectangle_7Répéterlemotdepasse"
        android:layout_width="354dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:width="354dp"
        android:height="36dp"
        android:background="@drawable/rectangle_7"
        android:viewportWidth="354"
        android:viewportHeight="36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Répéterlemotdepasse" />

    <!-- Comment avez-vous entendu parler de nous ? -->

    <EditText
        android:id="@+id/nom_et_pr_nTExtRépéterlemotdepasse"
        android:layout_width="332dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:gravity="top"
        android:text="Répétez le mot de passe"
        android:textColor="#66000000"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_7Répéterlemotdepasse"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_7Répéterlemotdepasse"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle_7Répéterlemotdepasse"
        app:layout_constraintTop_toTopOf="@+id/rectangle_7Répéterlemotdepasse"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/Comment"
        android:layout_width="390dp"
        android:layout_height="28dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:gravity="top"
        android:text="Comment avez-vous entendu parler de nous ?"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_7Répéterlemotdepasse" />

    <View
        android:id="@+id/rectangle_7Comment"
        android:layout_width="354dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:width="354dp"
        android:height="36dp"
        android:background="@drawable/rectangle_7"
        android:viewportWidth="354"
        android:viewportHeight="36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Comment" />

    <!-- Créer un compte -->
    <EditText
        android:id="@+id/nom_et_pr_nTExtComment"
        android:layout_width="330dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:gravity="top"
        android:text="Comment avez-vous entendu parler de nous ?"
        android:textColor="#66000000"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_7Comment"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_7Comment"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle_7Comment"
        app:layout_constraintTop_toBottomOf="@+id/Comment"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="309dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="24dp"
        android:width="171dp"
        android:height="50dp"
        android:background="@drawable/rectangle_4"
        android:onClick="launchAceuilPage"
        android:viewportWidth="171"
        android:viewportHeight="50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_7Comment" />

    <TextView
        android:id="@+id/connexionText"
        android:layout_width="200dp"
        android:layout_height="22dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:text="créer un compte"
        android:textColor="#F7F7F7"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_4"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_4"
        app:layout_constraintStart_toStartOf="@+id/rectangle_4"
        app:layout_constraintTop_toTopOf="@+id/rectangle_4" />

    <TextView
        android:id="@+id/vous_avez_d"
        android:layout_width="312dp"
        android:layout_height="17dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="16dp"
        android:gravity="center_horizontal|top"
        android:text="vous avez déjà un compte ?"
        android:textColor="#000000"

        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_4" />

    <!-- se connecter -->

    <View
        android:id="@+id/rectangle_5"
        android:layout_width="171dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="24dp"
        android:width="171dp"
        android:height="50dp"
        android:background="@drawable/rectangle_5"
        android:viewportWidth="171"
        android:viewportHeight="50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vous_avez_d" />

    <TextView
        android:id="@+id/connecterText"
        android:layout_width="135dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:text="Se connecter"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_5"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_5"
        app:layout_constraintStart_toStartOf="@+id/rectangle_5"
        app:layout_constraintTop_toTopOf="@+id/rectangle_5" />

</androidx.constraintlayout.widget.ConstraintLayout>